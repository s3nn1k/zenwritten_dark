{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#608B4E",
          "style": "plain",
          "template": "‚ùØ",
          "type": "text"
        },
        {
          "foreground": "#E5C07B",
          "style": "plain",
          "template": " {{ .UserName }}",
          "type": "session"
        },
        {
          "foreground": "#D4D4D4",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " <#F48771>in</> {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#D4D4D4",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <#F48771>git:(</>{{ .HEAD }}<#F48771>)</>",
          "type": "git"
        },
        {
          "foreground": "#E5C07B",
          "style": "plain",
          "template": " #",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
